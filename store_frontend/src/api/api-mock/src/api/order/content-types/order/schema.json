{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "orders"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "customer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::customer.customer"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "quantity": {
      "type": "integer"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "recebido",
        "processando",
        "separando",
        "enviado",
        "concluido"
      ]
    }
  }
}
